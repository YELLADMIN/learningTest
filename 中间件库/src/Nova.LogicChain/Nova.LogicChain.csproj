<Project Sdk="Microsoft.NET.Sdk">
  <!--引用解决方案公用项目配置-->
  <Import Project="../../build/ProjectCommon.targets" />
  
  <PropertyGroup>
    <RootNamespace>$(BaseLibraryName).LogicChain</RootNamespace>
    <AssemblyName>$(BaseLibraryName).LogicChain</AssemblyName>

    <Description>Nova库中的逻辑链库,基于netstandard2.0.结合建造者和逻辑链两种设计模式</Description>
    <PackageReleaseNotes>Nova库中的逻辑链库,基于netstandard2.0.结合建造者和逻辑链两种设计模式</PackageReleaseNotes>

    <!--库版本号-->
    <!--<Version>0.0.1.0</Version>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>-->

    <!--仓库地址-->
    <RepositoryUrl></RepositoryUrl>
    <PackageProjectUrl></PackageProjectUrl>

    <!--C#版本-->
    <LangVersion>7.3</LangVersion>

    <!--目标框架-->
    <TargetFramework>netstandard2.0</TargetFramework>

    <!--其它项目级别的配置-->

    <!--构建项目时生成Nuget包-->
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <!--是否验证许可证-->
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>

  </PropertyGroup>
  
  <PropertyGroup>
    <DocumentationFile>bin\$(Configuration)\$(BaseLibraryName).LogicChain.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>
    <OutputPath>bin\$(Configuration)</OutputPath>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
  </ItemGroup>


</Project>
