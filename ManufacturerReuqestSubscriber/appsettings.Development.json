{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
        //"Default": "Warning"
      }
    },

    /*
    这里是日志的配置，现在只配置了警告级别。
    有：0.Trace  1：Debug  2：Information  3：Warning  4：Error  5：Critical
     在项目根目录用dotnet run启动时才会加载这个文件
    */
    "Console": {
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Information",
      //"Default": "Warning",
      "System": "Information",
      "Microsoft": "Information"
    }

  }

}